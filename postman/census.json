      "name": "1. Setup & Management",
      "item": [
        {
          "name": "Import Catalog (Fast - No Metadata)",
          "request": {
            "method": "POST",
            "header": [{"key": "Content-Type", "value": "application/json"}],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"fetchMetadata\": false\n}"
            },
            "url": {
              "raw": "{{baseUrl}}/api/census/initial-import",
              "host": ["{{baseUrl}}"],
              "path": ["api", "census", "initial-import"]
            },
            "description": "Import ~2000 dataset catalog (2-5 minutes). No metadata."
          }
        },
        {
          "name": "Import Catalog (With Metadata - Slow)",
          "request": {
            "method": "POST",
            "header": [{"key": "Content-Type", "value": "application/json"}],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"fetchMetadata\": true,\n  \"maxDatasets\": 50,\n  \"batchSize\": 10,\n  \"vintageFilter\": [2022, 2023],\n  \"onlyAvailable\": true,\n  \"startIndex\": 0\n}"
            },
            "url": {
              "raw": "{{baseUrl}}/api/census/initial-import",
              "host": ["{{baseUrl}}"],
              "path": ["api", "census", "initial-import"]
            },
            "description": "Import catalog WITH metadata for specific datasets"
          }
        },
        {
          "name": "Sync Metadata (All Datasets)",
          "request": {
            "method": "POST",
            "header": [{"key": "Content-Type", "value": "application/json"}],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"limit\": 100,\n  \"batchSize\": 10,\n  \"delayMs\": 2000\n}"
            },
            "url": {
              "raw": "{{baseUrl}}/api/census/sync-metadata",
              "host": ["{{baseUrl}}"],
              "path": ["api", "census", "sync-metadata"]
            },
            "description": "Sync metadata for up to 100 datasets"
          }
        },
        {
          "name": "Sync Metadata (Specific Dataset)",
          "request": {
            "method": "POST",
            "header": [{"key": "Content-Type", "value": "application/json"}],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"identifier\": \"https://api.census.gov/data/id/ACSDT5Y2022\"\n}"
            },
            "url": {
              "raw": "{{baseUrl}}/api/census/sync-metadata",
              "host": ["{{baseUrl}}"],
              "path": ["api", "census", "sync-metadata"]
            },
            "description": "Sync metadata for one specific dataset"
          }
        },
        {
          "name": "Get Statistics",
